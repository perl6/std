    $self->{_from} = $S;
    if ($::ACTIONS) {
        eval { $::ACTIONS->$tag($self) };
        warn $@ if $@ and not $@ =~ /locate/;
    }
    $self->deb("REDUCE $tag from " . $S . " to " . $self->{_pos}) if $DEBUG & DEBUG::matchers;
    $self;
}

sub _COMMITBRANCH { my $self = shift;
