    $self->deb("Commit branch to $P") if $DEBUG & DEBUG::matchers;
    $self, LazyMap->new(sub { $self->deb("ABORTBRANCH") if $DEBUG & DEBUG::trace_call; die "ABORTBRANCH" }, $self);
}

