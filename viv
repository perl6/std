{ package VAST::infix; our @ISA = 'VAST::Base';
    sub emit_p5 {  my $self = shift; my $lvl = shift;
	my @t = $self->SUPER::emit_p5($lvl+1);
	if ($t[0] eq '+&' or $t[0] eq '~&') {
	    $t[0] = '&';
	}
	elsif ($t[0] eq '+|' or $t[0] eq '~|') {
	    $t[0] = '|';
	}
	$self->ret(@t);
    }
}

