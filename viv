}


{ package VAST::typename; our @ISA = 'VAST::Base';
    sub emit_p5 {  my $self = shift; my $lvl = shift;
	my @t;
	if (ref $context[-1] ne 'VAST::scoped') {
	    @t = $self->SUPER::emit_p5($lvl+1);
	}
        $self->ret($lvl,@t);
    }
}


