{ package VAST::xblock; our @ISA = 'VAST::Base';
    sub emit_p5 {  my $self = shift; my $lvl = shift;
	my @t = $self->SUPER::emit_p5($lvl+1);
	$t[0] = '(' . $t[0] . ')';
	$t[0] =~ s/(\s+)\)$/)$1/;
	$self->ret(@t);
    }
}

{ package VAST::XXX; our @ISA = 'VAST::Base';
    sub emit_p5 {  my $self = shift; my $lvl = shift;
	my @t = $self->SUPER::emit_p5($lvl+1);
	if ($t[0] eq 'XXX') {
	    $t[0] = 'XXX';
	}
	$self->ret(@t);
    }
}


if ($0 eq __FILE__) {
    ::MAIN(@ARGV);
}

